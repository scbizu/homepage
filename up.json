{
  "name": "homepage",
  "profile": "scnace_aws",
  "hooks": {
    "build":[
      "export GO111MODULE=on",
      "GOOS=linux GOARCH=amd64 go build -o server *.go"
    ],
    "clean":"rm server"
  },
  "proxy":{
    "command":"./server -s ./static/"
  },
  "stages":{
    "production":{
      "domain": "scnace.me"
    }
  },
  "regions": [
    "ap-southeast-1"
  ]
}
