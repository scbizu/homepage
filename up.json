{
  "name": "homepage",
  "profile": "scnace_aws",
  "hooks": {
    "build": [
      "GOOS=linux GOARCH=amd64 go build -o wasmserver server/main.go"
    ]
  },
  "proxy":{
    "command":"GO111MODULE=on ./wasmserver -workdir ./static github.com/scbizu/homepage/static "
  },
  "stages":{
    "production":{
      "domain":"scnace.me",
      "zone": false
    }
  },
  "regions": [
    "ap-southeast-1"
  ]
}
